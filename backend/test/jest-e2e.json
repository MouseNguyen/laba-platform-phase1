{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts"
    ],
    "rootDir": ".",
    "testEnvironment": "node",
    "testRegex": ".e2e-spec.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
        "^isomorphic-dompurify$": "<rootDir>/__mocks__/isomorphic-dompurify.ts"
    },
    "setupFilesAfterEnv": [
        "<rootDir>/setup.ts"
    ]
}